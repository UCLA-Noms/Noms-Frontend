{
  "private": true,
  "main": "entry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "lint": "node_modules/.bin/eslint src/ --fix src/ --fix",
    "test": "jest --passWithNoTests --silent --noStackTrace --runInBand",
    "test:verbose": "jest --passWithNoTests --runInBand --watch",
    "test:unit": "yarn test --watch -c ./jest-unit.config.js",
    "test:integration": "yarn test --watch -c ./jest-integration.config.js",
    "test:staged": "yarn test --findRelatedTests",
    "test:ci": "yarn test --coverage"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "~1.17.11",
    "@react-native-community/masked-view": "0.1.11",
    "@react-native-community/netinfo": "9.3.5",
    "@react-navigation/bottom-tabs": "^5.11.2",
    "@react-navigation/drawer": "^5.11.4",
    "@react-navigation/native": "^5.8.10",
    "@react-navigation/stack": "^5.12.8",
    "@reduxjs/toolkit": "^1.9.1",
    "@rneui/base": "^4.0.0-rc.7",
    "@rneui/themed": "^4.0.0-rc.7",
    "@stripe/stripe-react-native": "0.19.0",
    "axios": "^1.2.0",
    "babel-plugin-inline-dotenv": "^1.7.0",
    "expo": "^47.0.13",
    "expo-asset": "~8.7.0",
    "expo-auth-session": "~3.8.0",
    "expo-cli": "^6.2.1",
    "expo-dev-client": "~2.0.1",
    "expo-dev-menu": "^2.0.2",
    "expo-font": "~11.0.1",
    "expo-keep-awake": "~11.0.1",
    "expo-random": "~13.0.0",
    "expo-secure-store": "~12.0.0",
    "expo-updates": "~0.15.6",
    "expo-web-browser": "~12.0.0",
    "jest": "^29.4.0",
    "moment": "^2.29.2",
    "prop-types": "^15.7.2",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-native": "^0.70.5",
    "react-native-expo-viewport-units": "^0.0.8",
    "react-native-extended-stylesheet": "^0.12.0",
    "react-native-gesture-handler": "~2.8.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-reanimated": "~2.12.0",
    "react-native-safe-area-context": "4.4.1",
    "react-native-screens": "~3.18.0",
    "react-native-svg": "13.4.0",
    "react-native-svg-transformer": "^1.0.0",
    "react-native-vector-icons": "^6.6.0",
    "react-native-web": "~0.18.7",
    "react-redux": "^8.0.5",
    "redux": "^4.0.4",
    "redux-logger": "^3.0.6"
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-methods": "^7.18.6",
    "@babel/preset-env": "^7.20.2",
    "@types/expo": "^33.0.1",
    "@types/jest": "^29.4.0",
    "@types/react": "17.0.21",
    "@types/react-native": "~0.70.6",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "babel-eslint": "^10.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-preset-expo": "~9.2.1",
    "braces": ">=2.3.1",
    "eslint": "^7.20.0",
    "eslint-config-airbnb": "^17.0.2",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.32.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^4.2.5",
    "jest-expo": "^47.0.1",
    "lint-staged": "^13.0.4",
    "prettier": "^2.0.5",
    "pretty-quick": "^3.1.3",
    "react-test-renderer": "^18.0.0",
    "typescript": "^4.6.3"
  },
  "prettier": {
    "trailingComma": "all",
    "tabWidth": 2,
    "semi": false,
    "singleQuote": false,
    "bracketSpacing": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "jest": {
    "preset": "jest-expo",
    "setupFiles": [
      "<rootDir>/jestSetupFile.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/assetsTransformer.js",
      "\\.(css|less)$": "<rootDir>/assetsTransformer.js"
    },
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(react-clone-referenced-element|@react-native-community|react-navigation|@react-navigation/.*|@unimodules/.*|native-base|react-native-code-push)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/babel.config.js",
      "!**/jest.setup.js",
      "!metro.config.js",
      "!assetsTransformer.js",
      "!.eslintrc.js"
    ]
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "pretty-quick --staged",
      "yarn lint",
      "yarn test"
    ]
  },
  "browser": {
    "fs": false,
    "path": false,
    "os": false
  }
}